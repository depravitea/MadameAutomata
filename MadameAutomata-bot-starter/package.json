{
  "name": "madameautomata",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "start": "npm run prisma:gen && npm run prisma:push && npm run build && node dist/index.js",
    "register": "ts-node src/registerCommands.ts",
    "postinstall": "prisma generate",
    "prisma:gen": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8",
    "@prisma/client": "^5.22.0",
    "ioredis": "^5.4.1",
    "bullmq": "^5.7.8",
    "pino": "^9.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.22.0"
  }
}
